{
  "name": "beautifySvg",
  "displayName": "Prettier Svg",
  "description": "prettier and compress svg",
  "version": "0.0.1",
  "author": {
    "name": "GDYG",
    "email": "gdyg01@gmail.com"
  },
  "keywords": [
    "svg",
    "prettier",
    "compress"
  ],
  "publisher": "Ann",
   "repository": {
    "type": "git",
    "url": "https://github.com/GDYG/prettier-svg"
  },
  "homepage": "https://github.com/GDYG/prettier-svg?tab=readme-ov-file#readme",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "public/assets/images/svg.png",
  "contributes": {
    "commands": [
      {
        "command": "beautifySvg.prettierSvg",
        "title": "Prettier Svg"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "beautifySvg.prettierSvg",
          "when": "editorLangId == svg && editorHasSelection && !editorReadonly",
          "group": "navigation"
        }
      ]
    },
     "keybindings": [
      {
        "command": "beautifySvg.prettierSvg",
        "key": "ctrl+alt+g",
        "when": "editorLangId == svg && editorHasSelection && !editorReadonly",
        "mac": "cmd+alt+g"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
